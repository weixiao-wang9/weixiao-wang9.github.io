{
  "name": "my-portfolio",
  "type": "module",
  "version": "0.0.1",
"scripts": {
    "dev": "npm run prepare-content && astro dev",
    "start": "astro dev",
    "build": "npm run prepare-content && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare-content": "npm run sync && npm run fix-images",
    "sync": "bash sync.sh",
    "fix-images": "node fix-images.mjs",
    "publish": "npm run prepare-content && astro build && git add . && git commit -m \"Update content and fix image paths\" && git push"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.8",
    "globby": "^16.1.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "remark-obsidian-callout": "^1.5.1",
    "remark-wiki-link": "^2.0.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "zdog": "^1.1.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19"
  }
}
